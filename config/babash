set CONFIG_win32=-Dhost=win32 -Iconfig/win.gypi -Dinstall_prefix=$(PREFIX)
set LIBS_win32+=-Dheadless_lib=$(word 1,$(HEADLESS) cgl)
set LIBS_win32+=-Dplatform_lib=$(word 1,$(PLATFORM) win)
set LIBS_win32+=-Dasset_lib=$(word 1,$(ASSET) fs)
set LIBS_win32+=-Dhttp_lib=$(word 1,$(HTTP) nsurl)
set LIBS_win32+=-Dcache_lib=$(word 1,$(CACHE) sqlite)
set LIBS_win32+=--depth=. -Goutput_dir=.





gyp %CONFIG_win32% %LIBS_win32%
